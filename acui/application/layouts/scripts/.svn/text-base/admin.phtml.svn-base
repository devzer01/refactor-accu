<script type='text/javascript'>
	$(document).ready(function (e) {
		$('#addcourse').click(function (e) {
			e.preventDefault();
			//History.pushState(null, "Add Course", '/course/add');
			$('#content').html('').load('/course/add');
		});
		$('#listcourses').click(function (e) {
			e.preventDefault();
			//History.pushState(null, "List Courses", '/course/list');
			$('#content').html('').load('/course/list');
		});

		$('#addfacilitator').click(function (e) {
			$('#content').html('').load('/facilitator/add');
		});

		$('#addfacilitator').click(function (e) {
			$('#forum').html('').load('/discussion/index');
		});
		
		$('#listfacilitator').click(function (e) {
			$('#content').html('').load('/facilitator/list');
		});

		$('#addlearner').click(function (e) {
			$('#content').html('').load('/learner/add');
		})
		$('#listlearner').click(function (e) {
			$('#content').html('').load('/learner/list');
		});
		$('#assignments').click(function (e) {
			$('#content').html('').load('/assignment/list');
		});
		$('#evaluations').click(function (e) {
			$('#content').html('').load('/course/evaluations');
		});

		$("#progresstrack").click(function (e) {
			$("#content").html("").load("/assignment/progress");
		});

		$('#addeval').click(function (e) {
			$('#content').html('').load('/course/addeval');
		});

		$('#forum').click(function (e) {
			$('#content').html('').load('/discussion/index');
		});
		
	});
</script>            